{
  "name": "root",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_PRIVATE_LOCAL_WEBPACK=true next dev -p 5001",
    "build": "npm run lint && npm run format && cross-env NEXT_PRIVATE_LOCAL_WEBPACK=true next build  && cp -r .next/static .next/standalone/.next/ && cp -r public .next/standalone/",
    "start": "next start -p 5001",
    "prepare": "husky install",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\" --max-warnings=0",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,scss,css}\"",
    "docker:build:uat": "docker build -f Dockerfile.uat -t newwebtrade:uat .",
    "docker:build:prod": "docker build -f Dockerfile.production -t newwebtrade:prod .",
    "docker:build:krx": "docker build -f Dockerfile.krx -t newwebtrade:krx .",
    "docker:build:local": "docker build -f Dockerfile -t newwebtrade:local .",
    "docker:run:uat": "docker rm -f newwebtrade-uat 2>/dev/null || true && docker run -d -p 5001:5001 --name newwebtrade-uat newwebtrade:uat",
    "docker:run:prod": "docker rm -f newwebtrade-prod 2>/dev/null || true && docker run -d -p 5002:5001 --name newwebtrade-prod newwebtrade:prod",
    "docker:run:krx": "docker rm -f newwebtrade-krx 2>/dev/null || true && docker run -d -p 5003:5001 --name newwebtrade-krx newwebtrade:krx",
    "docker:run:local": "docker rm -f newwebtrade-local 2>/dev/null || true && docker run -d -p 5004:5001 --name newwebtrade-local newwebtrade:local",
    "docker:restart:uat": "npm run docker:build:uat && npm run docker:run:uat",
    "docker:clean": "docker ps -a | grep newwebtrade | awk '{print $1}' | xargs -r docker rm -f"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,scss,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource-variable/manrope": "^5.2.8",
    "@hookform/resolvers": "^5.2.1",
    "@microsoft/signalr": "^9.0.6",
    "@module-federation/nextjs-mf": "^8.8.12",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.8",
    "@mui/material-nextjs": "^6.4.3",
    "@reduxjs/toolkit": "^1.8.0",
    "@sentry/nextjs": "^10.10.0",
    "@sentry/profiling-node": "^10.10.0",
    "@types/react-virtualized": "^9.22.3",
    "antd": "^5.25.4",
    "axios": "^1.8.4",
    "cookies-next": "^4.3.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "flexlayout-react": "^0.7.15",
    "highcharts": "^12.2.0",
    "highcharts-react-official": "^3.2.2",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "lodash": "^4.17.21",
    "logrocket": "^10.1.0",
    "logrocket-react": "^6.0.3",
    "lottie-react": "^2.4.1",
    "mitt": "^3.0.1",
    "moment": "^2.30.1",
    "next": "^14.2.23",
    "prettier": "^3.6.2",
    "raw-body": "^3.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.1",
    "react-qr-code": "^2.0.15",
    "react-redux": "^8.0.0",
    "react-virtualized": "^9.22.6",
    "redux-saga": "^1.2.0",
    "sass": "^1.86.0",
    "sharp": "^0.33.5",
    "signalr-asp-net": "^1.0.3",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.19.25",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-grid-layout": "^1.3.5",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "typescript": "^5"
  }
}
